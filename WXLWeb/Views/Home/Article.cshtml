@{
    Layout = null;
}
@model WXLWeb.ViewModels.ArticleRead
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="Shortcut Icon" href="/Images/title.ico" />
    <title>@Model.article.Title -韦小龙个人博客</title>
    <link href="~/Content/Base.css" rel="stylesheet" />
    <link href="~/Plug/Ueditor/third-party/SyntaxHighlighter/shCoreDefault.css" rel="stylesheet" />
    <script src="http://libs.baidu.com/jquery/1.11.1/jquery.min.js "></script>
    <script src="~/Plug/Ueditor/third-party/SyntaxHighlighter/shCore.js"></script>
    <script src="~/Scripts/MyJs.js"></script>
    <link href="~/Content/Article.css" rel="stylesheet" />
    <script type="text/javascript">
        $(function () {
            SyntaxHighlighter.highlight();
        });
    </script>
</head>
<body>
    <div id="top">
        @{Html.RenderPartial("Head");}
        <div id="l_top">
        </div>
    </div>
    <div id="nav_txt">
        <div id="nav_txt_left">
            当前位置:<span class="sp_blank"></span><a href="@Url.Action("Index","Home")">首页</a><span class="sp_blank"></span>><span class="sp_blank"></span><span class="txtDefault">随笔分享</span>
        </div><div id="nav_txt_right">分享使人快乐~</div><div class="clear"></div>
    </div>
    <div id="s_body">
        <div id="s_body_left">
            <div class="sb_goback"><a href="javascript:window.history.go(-1);">返回</a></div>
            <div class="sb_Title"><a href="javascript:void(0);">@Model.article.Title</a></div>
            <div class="sb_info">
                发布时间:<span class="sp_blank"></span>@Model.article.CreateTime&nbsp;编辑:<a href="javascript:void(0);">@Model.article.UserName</a>&nbsp;
                阅读(@Model.article.LookNum)
            </div>
            <div class="sb_tag">
                @if (Model.tags.Count > 0)
                {
                    foreach (var ts in Model.tags)
                    {
                        <a class="sb_tag_a" href="javascript:void(0);">@ts.TagName</a>
                    }
                }
            </div>
            <div class="sb_content">
                @ViewBag.ContentTxt
            </div>
            <!-- JiaThis Button BEGIN -->
            <div class="jiathis_style" style="margin-top:10px">
                <span class="jiathis_txt">分享到：</span>
                <a class="jiathis_button_tools_1"></a>
                <a class="jiathis_button_tools_2"></a>
                <a class="jiathis_button_tools_3"></a>
                <a class="jiathis_button_tools_4"></a>
                <a href="http://www.jiathis.com/share" class="jiathis jiathis_txt jiathis_separator jtico jtico_jiathis" target="_blank">更多</a>
                <a class="jiathis_counter_style"></a>
                <script type="text/javascript" src="http://v3.jiathis.com/code/jia.js" charset="utf-8"></script>
                <!-- JiaThis Button END -->
            </div>
            <div class="clear"></div>
            <!--PC和WAP自适应版(畅言)-->
            <div id="SOHUCS" sid="@Model.article.ArticleId"></div>
            <script type="text/javascript">
                (function () {
                    var appid = 'cyt3cL2JJ';
                    var conf = 'prod_2a6e81a5f2e83a86f9f64848ec7f17db';
                    var width = window.innerWidth || document.documentElement.clientWidth;
                    if (width < 960) {
                        window.document.write('<script id="changyan_mobile_js" charset="utf-8" type="text/javascript" src="http://changyan.sohu.com/upload/mobile/wap-js/changyan_mobile.js?client_id=' + appid + '&conf=' + conf + '"><\/script>');
                    } else { var loadJs = function (d, a) { var c = document.getElementsByTagName("head")[0] || document.head || document.documentElement; var b = document.createElement("script"); b.setAttribute("type", "text/javascript"); b.setAttribute("charset", "UTF-8"); b.setAttribute("src", d); if (typeof a === "function") { if (window.attachEvent) { b.onreadystatechange = function () { var e = b.readyState; if (e === "loaded" || e === "complete") { b.onreadystatechange = null; a() } } } else { b.onload = a } } c.appendChild(b) }; loadJs("http://changyan.sohu.com/upload/changyan.js", function () { window.changyan.api.config({ appid: appid, conf: conf }) }); }
                })(); </script>

        </div>
        <div id="s_body_right">
            <h2 style="text-align:center">我的标签</h2>
            <p style="margin-left:10px;"><a href="javascript:void()">1.ASP.NET</a></p>
            <p style="margin-left:10px;"><a href="javascript:void()">2.SQL</a></p>
            <p style="margin-left:10px;"><a href="javascript:void()">3.ASP.NET MVC</a></p>
            <p style="margin-left:10px;"><a href="javascript:void()">4.EF</a></p>
            <p style="margin-left:10px;"><a href="javascript:void()">5.HTML5</a></p>
            <p style="margin-left:10px;"><a href="javascript:void()">6.CSS3</a></p>
            <p style="margin-left:10px;"><a href="javascript:void()">7.Bootstrap</a></p>
            <p style="margin-left:10px;"><a href="javascript:void()">8.Jquery/JS</a></p>
            <p style="margin-left:10px;"><a href="javascript:void()">9.Linq之路</a></p>
            <p style="margin-left:10px;"><a href="javascript:void()">10.EasyUI</a></p>
        </div>
        <div class="clear"></div>
    </div>
</body>
</html>
